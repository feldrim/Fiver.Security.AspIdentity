@model List<Fiver.Security.AspIdentity.Services.Identity.AppIdentityRole>

<h3>Roles</h3>

<table class="table table-hover">
    <thead class="thead">
        <tr>
            <th>Role name</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Description</td>
            </tr>
        }
    </tbody>
</table>